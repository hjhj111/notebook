ction(res){    \n    console.log(res.data)\n    self.notes=res.data\n    console.log(self.notes)  \n    }    \n    ).catch((err)=>{\n      console.log(err)      \n    })    \n    },\n\n    addNote: function(){\n      var self=this\n      console.log(document.getElementById(\"form\").value)\n      axios({\n        method:\"Post\",\n        url:self.address,\n        headers:{\n          'Content-Type': 'text/plain'//application/json会乱码\n        },\n        data:{text:document.getElementById(\"form\").value}\n        //data:\"{\\\"text\\\":\\\"更加发奋建安费\\\"}\"\n      }).then(function(res){\n        if(res.status==201){\n          //update\n          self.getNotes();\n          alert(\"添加完成\");\n        }\n        else{\n          alert(\"添加失败\");\n        }\n      })\n      \n    }\n    \n  }\n}\n\n\n\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n\n</style>\n"]},"metadata":{},"sourceType":"module"}